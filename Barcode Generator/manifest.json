{
    "manifest_version": 3,
    "name": "Barcode Generator Extension JS",
    "version": "1.0",
    "description": "Simple Barcode generator extension built in pure HTML, CSS, and JS.",
    "author": "sambitmondal2005@gmail.com",
    "icons": {
        "48": "images.png",
        "128": "images.png"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "index.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}
